{
    "name": "chat/whatsapp-integration",
    "description": "A package for integrating WhatsApp features into Laravel 5.8 applications.",
    "type": "library",
    "keywords": [
        "laravel",
        "whatsapp",
        "twilio",
        "messaging"
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.8",
        "illuminate/cache": "^5.8",
        "illuminate/container": "^5.8",
        "twilio/sdk": "^8.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Chat\\WhatsappIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chat\\WhatsappIntegration\\WhatsAppIntegrationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Agnes",
            "email": "agypeter97@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "version": "2.0.3",
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}