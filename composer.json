{
    "name": "chat/whatsapp-integration",
    "description": "A package for integrating WhatsApp features into Laravel 5.8 applications.",
    "type": "library",
    "require": {
        "illuminate/support": "^5.8",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^7.5",
        "twilio/sdk": "^8.3",
        "vlucas/phpdotenv": "^3.3"
    },
    
    "autoload": {
        "psr-4": {
            "Chat\\WhatsappIntegration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chat\\WhatsappIntegration\\WhatsAppIntegrationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Agnes",
            "email": "agypeter97@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "mockery/mockery": "^1.3"
    },
    "version": "1.0.5"
}
